# Environment Configuration Template
# Copy this file to .env and update with your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Health Data Export Directories
HEALTH_EXPORT_DIR=/path/to/health/exports
HEALTH_TZ=America/Los_Angeles

# MyFitnessPal Export
MFP_CSV=/path/to/mfp/export.csv

# TrainingPeaks Export
TRAININGPEAKS_CSV=/path/to/trainingpeaks/export.csv

# Withings Export
WITHINGS_CSV=/path/to/withings/export.csv

# Health Auto Export (HAE)
HAE_JSON_DIR=/path/to/hae/json/exports

# API Configuration
PORT=3001
NODE_ENV=development

# Add other non-sensitive config examples as needed
